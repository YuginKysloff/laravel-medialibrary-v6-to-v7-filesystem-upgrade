{
    "name": "spatie/laravel-medialibrary-v6-to-v7-filesystem-upgrade",
    "description": "This tool will add the original name to the comvertions like specified in the upgrade guide of the medialibrary version 7",
    "keywords": [
        "spatie",
        "laravel-medialibrary-v6-to-v7-filesystem-upgrade"
    ],
    "homepage": "https://github.com/spatie/laravel-medialibrary-v6-to-v7-filesystem-upgrade",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-zero/laravel-zero/issues",
        "source": "https://github.com/laravel-zero/laravel-zero"
    },
    "authors": [
        {
            "name" : "Thomas Verhelst",
            "email" : "tvke91@gmail.com",
            "homepage" : "https://spatie.be",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel-zero/framework": "5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["upgrade-tool"]
}
