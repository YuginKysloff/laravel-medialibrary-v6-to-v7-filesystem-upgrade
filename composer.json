{
    "name": "spatie/medialibrary-v7-upgrade-tool",
    "description": "This tool will add the original name to the comvertions like specified in the upgrade guide of the medialibrary version 7",
    "keywords": [
        "spatie",
        "medialibrary-v7-upgrade-tool"
    ],
    "homepage": "https://github.com/spatie/medialibrary-v7-upgrade-tool",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Verhelst",
            "email": "tvke91@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/console": "~5.5.0|~5.6.0",
        "tightenco/collect": "^5.6"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "orchestra/testbench" : "^3.6",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\UpgradeTool\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\UpgradeTool\\Tests\\" : "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\UpgradeTool\\UpgradeToolServiceProvider"
            ]
        }
    }
}
